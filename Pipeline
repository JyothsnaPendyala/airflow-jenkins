pipeline{
    agent any
    environment{
        airflow_instance_ip = credentials('airflow_instance_ip')
        airflow_instance_private_key = credentials('airflow_instance_private_key')
    }
    stages{
        stage("print env"){
            steps{
                 sh "printenv"
                }
            }
        stage("Trigger Airflow Dag"){
            steps{
                 sh "python3 trigger_airflow_dag.py"
                }
            }
        }
}
