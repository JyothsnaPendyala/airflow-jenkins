pipeline{
    agent any
    environment{
        airflow_instance_ip = credentials('airflow_instance_ip')
        airflow_instance_private_key = credentials('airflow-ec2-user')
    }
    stages{
        
        stage("Trigger Airflow Dag"){
            steps{
                 sh "python3 trigger_airflow_dag.py"
                }
            }
        }
}
